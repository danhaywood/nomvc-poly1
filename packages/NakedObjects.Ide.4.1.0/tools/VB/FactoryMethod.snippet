<?xml version="1.0"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Factory Method</Title>
      <Author>Naked Objects Group</Author>
      <Description>Adds a factory method that returns a new instance of a specified type.</Description>
      <HelpUrl>www.nakedobjects.net</HelpUrl>
      <Shortcut>fact</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>Name</ID>
          <Default>CreateNew</Default>
        </Literal>
        <Literal>
          <ID>Type</ID>
          <Default>Type</Default>
        </Literal>
      </Declarations>
      <Code Language="VB">
    <![CDATA['Create new $Type$
Public Function $Name$$Type$() As $Type$
	Dim my$Type$ As $Type$= Container.NewTransientInstance(Of $Type$)
  With my$Type$
	'set up any parameters
  End With
	'Container.Persist(my$Type$)
	Return my$Type$
End Function]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>