<?xml version="1.0"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Title</Title>
      <Author>Naked Objects Group</Author>
      <Description>Adds a Title() method to function as the domain object's title.</Description>
      <HelpUrl>www.nakedobjects.net</HelpUrl>
      <Shortcut>title</Shortcut>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>NakedObjects</Namespace>
        </Import>
      </Imports>
      <Declarations>
        <Literal>
          <ID>content</ID>
        </Literal>
      </Declarations>
      <Code Language="VB" Kind="method decl"><![CDATA[Public Function Title() as String
	Dim t as new TitleBuilder()
	t.append($content$)
	Return t.ToString()
End Function]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>